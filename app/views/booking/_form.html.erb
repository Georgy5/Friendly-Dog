<%= simple_form_for(@booking, @dog) do |f| %>
  <%= f.error_notification %>

  <!-- # TODO  -->
  # user's name @user.name
  <%= f.input :usersname, required: true %>
  # dog's name @user.name
  <%= f.input :city, required: true %>
  # Date to rent dog @booking.date
  <%= f.input :city, required: true %>

  <div class="form-inputs">
    <%= f.input :name,
                required: true,
                autofocus: true %>
    <%= f.input :breed,
                required: true %>

    <%= f.input :city, required: true %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
<%= simple_form_for [@dog, @booking] do |f| %>
  <%= f.error_notification %>

  <%= f.input :date, input_html: {class: "datepicker"} %>
  <!-- %= f.input :date,
   placeholder: "yyyy-mm-dd" % -->

  <div class="form-actions">
    <%= f.button :submit, "Submit", class: "btn btn-primary" %>
  </div>
<% end %>